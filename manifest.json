{
  "name": "KMS Twitter",
  "version": "1.0.1",
  "description": "A variety of UI improvements for Twitter.",
  "content_scripts": [{
    "matches": ["*://*.twitter.com/*"],
    "css": ["assets/css/kms_twitter.css"],
    "js": ["assets/js/main.js"],
    "run_at": "document_start"
  }],
  "icons": { 
    "48": "icon48.png",
    "128": "icon128.png" 
 },
 "web_accessible_resources": [
     "assets/images/star-filled.png",
     "assets/images/star.png"
  ],
  "manifest_version": 2
}
